<div>
    <h1>Register</h1>
</div>
<form action="/api/sessions/register" method="post" class="register">

<label for="">FirstName</label>
<input type="text" placeholder="name" id="name-f" name="firts_name" required>
<label for="">LastName</label>
<input type="text" placeholder="lastname" id="last-f" name="last_name" required>
<label for="">Email</label>
<input type="email" placeholder="email" id="email-f" name="email_user" required>
<label for="">Age</label>
<input type="number" placeholder="age" id="ager-f" name="age_user" required>
<label for="">Password</label>
<input type="password" placeholder="password" id="password-f" name="password_user" required>
<div>
    <button type="submit">Register</button>
    

</div>
</form>